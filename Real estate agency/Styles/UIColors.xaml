<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--Background Colors-->
    <Color x:Key="primaryBackColor1">#E6E6FA</Color>
    <Color x:Key="primaryBackColor2">#F0F0FF</Color>
    <Color x:Key="secondaryBackColor1">#F5F5FF</Color>
    <Color x:Key="secondaryBackColor2">#FAFAFF</Color>

    <!--Window Border Colors-->
    <Color x:Key="winBorderColor1">#6D2FFF</Color>
    <Color x:Key="winBorderColor2">#FB539B</Color>
    <Color x:Key="winBorderColor3">#836EFB</Color>

    <!--Color Palette-->
    <SolidColorBrush x:Key="color1" Color="#E285DE"/>
    <SolidColorBrush x:Key="color2" Color="#784DFD"/>
    <SolidColorBrush x:Key="color3" Color="#4ADAEC"/>
    <SolidColorBrush x:Key="color4" Color="#FB539B"/>
    <SolidColorBrush x:Key="color5" Color="#7E82FC"/>
    <SolidColorBrush x:Key="color6" Color="#FFC047"/>
    <SolidColorBrush x:Key="color7" Color="#EF6C96"/>
    <SolidColorBrush x:Key="color8" Color="#78A3FC"/>
    <SolidColorBrush x:Key="color9" Color="#07F3C0"/>
    <SolidColorBrush x:Key="color10" Color="#FBA1AA"/>

    <!--Button Colors-->
    <SolidColorBrush x:Key="buttonColor1" Color="#FFC1C1E2"/>
    <SolidColorBrush x:Key="buttonOver1" Color="#FFA9A9DE"/>
    
    <!--Table Colors-->
    <SolidColorBrush x:Key="PrimaryColor" Color="#FFDADAFB"/>
    <SolidColorBrush x:Key="SecondaryColor" Color="#F8F8FF"/>
    <SolidColorBrush x:Key="HighlightColor" Color="#F5F5FF"/>
    <SolidColorBrush x:Key="SelectColor" Color="#F5F5FF"/>

    <!--ScrolBar Colors-->
    <SolidColorBrush x:Key="ScrolBarColor1" Color="#FFCFCFFB"/>
    <SolidColorBrush x:Key="ScrolBarColor2" Color="#FFC1C1E2"/>
    
    <!--ComboBox Colors-->
    <SolidColorBrush x:Key="ComboBoxColor" Color="#FFCFCFFB"/>
    
    <!--Font Colors-->
    <SolidColorBrush x:Key="titleColor1" Color="#000000"/>
    <SolidColorBrush x:Key="titleColor2" Color="#333333"/>
    <SolidColorBrush x:Key="titleColor3" Color="#666666"/>
    <SolidColorBrush x:Key="plainTextColor1" Color="#999999"/>
    <SolidColorBrush x:Key="plainTextColor2" Color="#666666"/>
    <SolidColorBrush x:Key="plainTextColor3" Color="#333333"/>

    <!--Panel Colors-->
    <SolidColorBrush x:Key="panelColor" Color="#F5F5FF"/>
    <SolidColorBrush x:Key="panelOverColor" Color="#FFE6E2FD"/>
    <SolidColorBrush x:Key="panelActiveColor" Color="#FFD6D6FD"/>

</ResourceDictionary>